{
  "name": "Test Chain Hide ChatBubble v1.0",
  "plugins": ["jsonlogic", "obs"],
  "links": [
    {
      "plugin": "obs",
      "event": "CurrentProgramSceneChanged"
    },
    {
      "plugin": "jsonlogic",
      "link": "evaluate",
      "params": {
        "!==": ["{event.sceneName}", "Scene"]
      }
    },
    {
      "plugin": "obs",
      "link": "SetSceneItemEnabled",
      "params": {
        "sceneName": "Scene",
        "sceneItemEnabled": false,
        "sourceName": "ChatBubble"
      }
    }
  ]
}
